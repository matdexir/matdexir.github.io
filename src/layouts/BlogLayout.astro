---
import Layout from "./Layout.astro";
import Content from "../components/Content.astro";
export interface Props {
  title: string;
  description?: string;
  frontmatter?: any;
}

const { title, description = "" } = Astro.props;
// const { ToC } = Astro.props.frontmatter;
// const { minuteRead } = Astro.props.frontmatter;
---

<Layout title={title} description={description}>
  <Content
		classname="mt-10 prose dark:prose-invert prose-headings:text-emerald-700 prose-pre:mx-auto prose-pre:ring-1 prose-pre:ring-emerald-700 marker:text-emerald-700"
  >
    <slot />
  </Content>
</Layout>
