---
import Content from "../components/Content.astro";
import Banner from "../components/Banner.astro";
import Layout from "../layouts/Layout.astro";
import * as Intro from "./about_author/intro.md";
---

<Layout title="Matdexir's page">
  <Banner />
  <Content class="mt-2">
    <article
      class="prose dark:prose-invert prose-headings:text-green-600 prose-a:text-green-600 prose-a:no-underline"
      set:html={Intro.compiledContent()}
    />
  </Content>
</Layout>
